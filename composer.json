{
  "name": "benmatselby/trello-cli",
  "description": "Small Trello CLI application for creating sprint boards based and management tools",
  "require": {
    "symfony/console": "^6.1",
    "guzzlehttp/guzzle": "7.7.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "phpstan/phpstan": "^1.2",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "config": {
    "bin-dir": "bin/"
  },
  "autoload": {
    "psr-4": {
      "TrelloCli\\": "src/"
    }
  }
}
